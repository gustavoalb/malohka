<%= simple_form_for @nivel, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :nome %>
  <%= f.input :codigo %>
  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                niveis_path, :class => 'btn btn-default' %>
<% end %>