<!-- oi -->
<div class="page-header" align='center'>
  <h1>Iteração</h1>
</div>
<%= abre_estilo_12_well %>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Id</th>
      <th>Nome</th>
      <th>Criado em</th>
      <th>Status</th>
      <th>Ações</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= link_to @iteracao.id, iteracao_path(@iteracao) %></td>
      <td><%= @iteracao.nome %></td>
      <td><%= @iteracao.created_at.strftime("%d/%m/%y às %H:%M") %></td>
      <td><%= @iteracao.status %></td>
      <td>
        <a data-toggle="collapse" data-parent="#accordion" href="#sanfonaum"><span class="glyphicon glyphicon-resize-full" aria-hidden="true"></span> oi</a>
        <a data-toggle="collapse" data-parent="#accordion" href="#sanfonaum"><span class="glyphicon glyphicon-resize-full" aria-hidden="true"></span> Expandir</a></td>
    </tbody>
  </table>
  <div class="panel panel-default">
    <div id="sanfonaum" class="panel-collapse collapse">
      <div class="col-sm-12 col-md-12, row, well">
        <table class="table table-striped table-hover table-bordered table-condensed">
          <tr>
            <th>Tarefa</th>
            <th>Gravidade</th>
            <th>Urgência</th>
            <th>Tendência</th>
            <th>Total</th>
            <th>Status</th>
          </tr>
          <%# @iteracao.guts.each do |gut|%>
            <%# Iteracao.with_cd_players.each do |t|%>
              <% Gut.pendentes.each do |gut|%>
                <tr>
                  <td><%= gut[:item] %></td>
                  <td><%= gut[:gravidade] %></td>
                  <td><%= gut[:urgencia] %></td>
                  <td><%= gut[:tendencia] %></td>
                  <td><%= gut.total_gut %></td>
                  <td><%= gut.status %></td>
                <% end %><%# end %>
              </tr>
            </table>
            <p align='center'><a data-toggle="collapse" data-parent="#accordion" href="#sanfonadois">legenda</a></p>
            <div>
              <%= render 'avaliados_gut' %>
            </div>
            <p align='center'><a data-toggle="collapse" data-parent="#accordion" href="#sanfonatres">Ver legenda</a></p>
            <div>
              <%= render 'matriz_gut' %>
            </div>
          </div>
        </div>
      </div>
      <%= fecha_estilo_12_well %>
      <%= link_to t('.back', :default => t("helpers.links.voltar")),
              iteracoes_path, :class => 'btn btn-default'  %>
      <%= link_to t('.edit', :default => t("helpers.links.editar")),
              edit_iteracao_path(@iteracao), :class => 'btn btn-default' %>
      <%= link_to t('.destroy', :default => t("helpers.links.apagar")),
              iteracao_path(@iteracao),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>