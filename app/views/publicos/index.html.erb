<p id="notice"><%= notice %></p>

<h1>Listing Publicos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publicos.each do |publico| %>
      <tr>
        <td><%= publico.nome %></td>
        <td><%= publico.descricao %></td>
        <td><%= link_to 'Show', publico %></td>
        <td><%= link_to 'Edit', edit_publico_path(publico) %></td>
        <td><%= link_to 'Destroy', publico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publico', new_publico_path %>
