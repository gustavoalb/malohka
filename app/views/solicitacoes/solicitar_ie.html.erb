<%= simple_form_for @solicitacao,url: solicitacoes_url(tipo:'ie') do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :solicitante_id, collection: @pessoa.alunos.collect{|a|[a.matricula,a.id]} %>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>