<%= simple_form_for(@departamento) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nome %>
    <%= f.input :sigla %>
    <%= f.input :reparticao_id %>
    <%= f.input :data_criacao %>
    <%= f.input :portaria_criacao %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
